<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local_cvt="clr-namespace:ArcGridOptimizer.Cvts"
                    xmlns:local_enum="clr-namespace:ArcGridOptimizer.Enums"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    
    <ObjectDataProvider x:Key="GemPropertyValues"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="local_enum:eGemProperty"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="CoreTypePropertyValues"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="local_enum:eCoreGrade"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="CoreEffLevelValues"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="local_enum:eCoreEffectLevelGainType"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <local_cvt:CoreGradeToColorConverter x:Key="CoreGradeToColorCvt"/>
    <local_cvt:Enum2DescCvt x:Key="Enum2DescCvt"/>
    <local_cvt:GemPropToStringCvt x:Key="GemPropToStringCvt"/>
</ResourceDictionary>